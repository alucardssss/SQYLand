{% if is_granted('ROLE_ADMIN') %}
    <form method="post" action="{{ path('projet_delete', {'id': projet.id}) }}" onsubmit="return confirm('Voulez vous vraiment supprimer ce projet ?');">
        <input type="hidden" name="_method" value="DELETE">
        <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ projet.id) }}">
        <button class="btn btn-danger supericon" title="Supprimer ce projet">&#128501;</button>
    </form>
{% endif %}